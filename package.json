  "main": "index.js",
  "scripts": {
    "start": "live-server --open=dist --wait=50",
    "eslintFix": "eslint --fix --ext .js,.jsx .",
    "csslintFix": "stylelint --fix src/styles/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinityspiral/retouching-calculator.git"
  },
  "keywords": [
    "pug",
    "sass",
    "scss",
    "autoprefixer",
    "autoprefixer",
    "html"
  ],
  "author": "Chris Norton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/infinityspiral/retouching-calculator/issues"
  },
  "homepage": "https://github.com/infinityspiral/retouching-calculator#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "stylelint": "^13.6.1",
    "stylelint-scss": "^3.18.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "normalize.css": "^8.0.1"
  }
